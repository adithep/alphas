

@import 'alpmixins'

.form_nav
  float center
  padding 40px
  display inline-block
  border-radius 5px
  -webkit-border-radius 5px
  margin 0 auto

.form_nav ul
  padding 0
  margin 0
  list-style-type: none

.form_nav li
  display inline-block
.form_nav a
  background $brown
  padding 10px
  color $text-white
