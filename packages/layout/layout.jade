head
  meta(name="viewport", content="width=device-width, initial-scale=1.0")
  title αSΨS

body
  if currentUser
    +layout
  else
    +authentication

template(name="layout")
  div(id="parent")
    nav(class="top-bar", data-topbar="")
      ul(class="title-area")
        li(class="name")
          h1
            a(href="/") αSΨS
        li(class="toggle-topbar menu-icon")
          a
            span Menu
      section(class="top-bar-section")
        ul(class="left")
          li(class="divider")
          li
            a(data-path="an", href="/module_test") moduleTest
          li
            a(data-path="an", href="/add_contact") Add Contact
          li
            a(data-path="an", href="/display_humans") Display Contact
        ul(class="right")
          li(class="divider")
          li(class="has-dropdown")
            a User : #{currentUser.username}
            ul(class="dropdown")
              li
                a(id="logout") logout
    div(class="row", id="yieldchild")
      div(class="large-12 columns")
        +alpha_yield

