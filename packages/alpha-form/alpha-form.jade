template(name="alpha_form")

  a(class="save_human_json right") Save Json
  nav(class="form_nav")
    ul
      each schema_buttons
        +_schema_buttons

  form(class="contact_form")
    each input_element
      +_each_input_master
    br
    a(class="form_submit") Submit

template(name="_each_input_master")
  div(class="_each_input_master")
    each this._tri
      +_each_input

template(name="_each_input")
  div(class="#{this.class_n} contact_div")
    span(class="s_liga") ÃŽ
    +UI.dynamic template=tmpl data=tmpl_data


template(name="_schema_buttons")
  li(class="#{this.class_n}")
    a
      each get_key_dis
        +_key_dis

template(name="_key_dis")
  span #{this.key_dis}

template(name="_string_input")
  input(placeholder="#{this.key_dis}", type="#{input_type}", class="input_ui")

template(name="_string_select")
  input(type="text", class="input_ui input_select", value="#{select_value}")
  div(class="div_select")
    each select_options ../../../../_id
      +_string_select_options

template(name="_string_select_options")
  span(class="input_ui") {{h_opt ../key_key}}

template(name="_a_select_li")
  li
    a #{this.city._v} #{this.country._v}
